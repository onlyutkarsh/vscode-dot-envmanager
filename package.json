{
  "name": "envmanager",
  "displayName": ".env Manager",
  "description": "Quick and easy way to add entries to your .env file",
  "publisher": "onlyutkarsh",
  "author": "Utkarsh Shigihalli",
  "version": "0.0.1",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#F2F2F2",
    "theme": "light"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onlyutkarsh/envmanager"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    ".env",
    "environment",
    "development",
    "productivity"
  ],
  "license": "gpl-3.0",
  "activationEvents": [
    "onCommand:envmanager.addToEnv"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    },
    "virtualWorkspaces": {
      "supported": false
    }
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "envmanager.addToEnv",
        "title": ".env Manager: Add to .env"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "envmanager.addToEnv",
          "group": "envManagerMenus@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run -S esbuild-base -- --minify",
    "esbuild-base": "rimraf out && esbuild ./src/extension.ts --tsconfig=src/tsconfig.json --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run -S esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run -S esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./src",
    "dev": "npm run compile && npm run esbuild-watch",
    "package": "vsce package",
    "deploy": "vsce publish",
    "clean": "git clean -fdX",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.55.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "esbuild": "^0.14.48",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vsce": "^2.9.2"
  }
}
